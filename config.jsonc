{
  "modules-left": ["custom/window-title"],
  "modules-right": [
    "custom/fish",
    "custom/cc",
    "custom/nekoviewer",
    "custom/tailscale",
    "custom/gpu_memory",
    "memory"
  ],
  "clock": {
    "format": "{:%A %d %B %Y %H:%M}",
    "tooltip": false,
    "on-click":"omarchy-launch-floating-terminal '/home/jebin/git/omarchy_custom_config/clock/target/release/clock'",
    "on-click-right": "omarchy-cmd-tzupdate"
  },
  "memory": {
    "interval": 5,
    "format": "RAM: {used:0.1f}G/{total:0.1f}G  "
  },
  "custom/gpu_memory": {
    "exec": "/home/jebin/git/omarchy_custom_config/vram.sh",
    "interval": 5,
    "on-click": "omarchy-launch-floating-terminal '/home/jebin/git/omarchy_custom_config/show_nvidia.sh'"
  },
  "custom/window-title": {
    "exec": "bash -c 'prev=\"\"; while true; do title=$(hyprctl activewindow | awk -F\": \" \"/title:/ {print \\$2}\"); title=\"${title:-No window}\"; if [[ \"$title\" != \"$prev\" ]]; then echo \"$title\" | cut -c1-50; prev=\"$title\"; fi; sleep 0.5; done'",
    "interval": 0,
    "format": "{}",
    "on-click": "omarchy-launch-floating-terminal '/home/jebin/git/omarchy_custom_config/HyprlandWindowSwitcher/target/release/hypr_window_switcher'"
  },
  "custom/tailscale": {
    "exec": "/home/jebin/git/omarchy_custom_config/tailscale.sh",
    "interval": 10,
    "return-type": "plain",
    "format": "{}",
    "on-click": "omarchy-launch-floating-terminal 'sudo tsui'",
    "tooltip": false
  },
  "custom/nekoviewer": {
    "exec": "/home/jebin/git/neko-apps/neko-viewer/main.sh 1",
    "interval": 10,
    "return-type": "plain",
    "format": {},
    "on-click": "/home/jebin/git/neko-apps/neko-viewer/main.sh 2",
    "tooltip-format": "localhost:8731"
  },
  "custom/fish": {
    "exec": "/home/jebin/git/omarchy_custom_config/fish_anim.sh",
    "restart-interval": 1,  // Keep it alive
    "tooltip": false
  },
  "custom/cc": {
    "exec": "/home/jebin/git/omarchy_custom_config/captioncreator.sh 1",
    "interval": 10,
    "return-type": "plain",
    "format": {},
    "on-click": "/home/jebin/git/omarchy_custom_config/captioncreator.sh 2",
    "tooltip-format": "localhost:3000"
  }
}
